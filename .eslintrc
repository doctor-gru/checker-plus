{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "endOfLine": "auto"
    }],
    "no-console": "off"
  }
}
